{
  "name": "dad-debt-tracker",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "frontend": "cd frontend && npm run dev",
    "frontend-build": "cd frontend && npm run build",
    "preview": "cd frontend && npm run preview",
    "frontend-build-only": "cd frontend && npm run build-only",
    "type-check": "cd frontend && npm run type-check",
    "lint": "eslint . --fix",
    "server": "cd backend && npm run nodemon",
    "server-build": "cd backend && npm run build",
    "server-prod": "cd backend && npm run production",
    "format": "prettier --write frontend/src/ && prettier --write backend/src/"
  }
}
