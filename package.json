{
  "name": "dad-debt-tracker",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "frontend": "cd frontend && npm run dev",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-server": "tsc -p backend/tsconfig.json && tsc-alias -p tsconfig.node.json",
    "server-prod": "npm run build-server && node dist/api/server.js",
    "server": "npx tsx backend/src/server.ts",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write frontend/src/"
  },
  "dependencies": {
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "esbuild": "^0.25.1",
    "express": "^4.21.2",
    "js-cookie": "^3.0.5",
    "mongodb": "^6.14.2",
    "mongoose": "^8.12.1",
    "path-to-regexp": "^8.2.0",
    "pinia": "^3.0.1",
    "tsx": "^4.19.3",
    "undici": "^7.5.0",
    "uuid": "^11.1.0",
    "vercel": "^41.4.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@tsconfig/node22": "^22.0.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.13.10",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.21.0",
    "eslint-plugin-vue": "^9.30.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.11",
    "typescript": "~5.6.3",
    "vite": "^6.0.5",
    "vite-plugin-vue-devtools": "^7.6.8",
    "vue-tsc": "^2.1.10"
  }
}
